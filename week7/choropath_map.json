{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 240,
  "data": {
    "url": "https://vega.github.io/vega-datasets/data/us-10m.json",
    "format": {
      "type": "topojson",
      "feature": "counties"
    }
  },
  "transform": [
    {
      "lookup": "id",
      "from": {
        "data": {
          "url": "https://vega.github.io/vega-datasets/data/unemployment.tsv"
        },
        "key": "id",
        "fields": ["rate"]
      }
    }
  ],
  "projection": {
    "type": "albersUsa"
  },
  "mark": {
    "type": "geoshape",
    "stroke": "black"
  },
  "encoding": {
    "color": {
      "field": "rate",
      "type": "quantitative",
      "title": "Unemployment Rate"
    },
    "tooltip": [
      {
        "field": "properties.name",
        "type": "nominal",
        "title": "Place Name"
      },
      {
        "field": "rate",
        "type": "quantitative",
        "title": "Unemployment Rate"
      }
    ]
  }
}
