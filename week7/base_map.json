{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 400,
  "data": {
    "url": "https://vega.github.io/vega-datasets/data/us-10m.json",
    "format": {
      "type": "topojson",
      "feature": "counties"
    }
  },
  "projection": {
    "type": "albersUsa",
    "scale": 1000
  },
  "mark": {
    "type": "geoshape",
    "stroke": "black"
  },
  "config": {
    "view": {
      "stroke": null
    },
    "events": {
      "zoom": true
    }
  }
}
