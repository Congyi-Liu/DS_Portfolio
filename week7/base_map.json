{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 300,
  "data": {
    "url": "data/us-10m.json",
    "format": {
      "type": "topojson",
      "feature": "states"
    }
  },
  "projection": {
    "type": "albersUsa"
  },
  "mark": {
    "type": "geoshape",
    "stroke": "white"
  },
  "encoding": {
    "tooltip": [
      {
        "field": "properties.name",
        "type": "nominal",
        "title": "State"
      }
    ]
  }
}
