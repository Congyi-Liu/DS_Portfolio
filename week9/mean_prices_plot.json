{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 600,
  "data": {
    "url": "db_region.csv",
    "format": {
      "type": "csv"
    }
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "region",
      "type": "nominal",
      "axis": {
        "labelAngle": 45,
        "labelAlign": "right"
      }
    },
    "y": {
      "field": "p_mean",
      "type": "quantitative",
      "title": "Mean Price (\u00a3)"
    },
    "color": {
      "value": "#90ee90"
    }
  },
  "title": "Mean Prices by Region"
}